{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp}[{Level}] - Message: {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=.;Database=BatchDB;Trusted_Connection=True;MultipleActiveResultSets=True;TrustServerCertificate=true",
          "tableName": "Logs"
        }
      }
    ]
  },

  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=BatchDB;Trusted_Connection=True;MultipleActiveResultSets=True;TrustServerCertificate=true"
  },
  "BatchUpload": "BatchUpload",
  "StorageConnections": "DefaultEndpointsProtocol=https;AccountName=storage10801;AccountKey=7yyblZRuF7dNK61XdgKz++2Jcq8cIHAVZ7vtd+H1Bpj8b5qDopBRGnw9WaMNHPUorEQne2iOFfrs+AStXzKgzg==;EndpointSuffix=core.windows.net",

  "KeyVaultURL": "https://batchsecrets.vault.azure.net",
  "StorageConnectionsSecret": "AppSecrete",
  "DbConnectionStringSecret": "BatchDbConnectionString"
  
}
